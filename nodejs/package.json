{
	"name": "@speechify/api-sdk",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"type": "module",
	"files": [
		"/lib",
		"/src"
	],
	"exports": {
		".": {
			"require": {
				"default": "./lib/cjs/index.js",
				"types": "./lib/cjs/index.d.ts"
			},
			"import": {
				"default": "./lib/esm/index.js",
